<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                  xmlns:views="clr-namespace:Google_sheetAndro.Views"
                  mc:Ignorable="d"
             x:Class="Google_sheetAndro.Views.MenuPage">
    <MasterDetailPage.Master>
        <ContentPage Title="Master">
            <ContentPage.Content>
                <StackLayout Padding="5,5,5,5">
                    <Button x:Name="ItemsPg" Text="Просмотр записей"
                            Clicked="ItemPageView_Clicked"/>
                    <Button x:Name="OutPg" Text="Граф Отчёт"
                            Clicked="GetItems_Clicked"/>
                    <Button x:Name="MapPg" Text="Картография"
                            Clicked="GetItems_Clicked_2"/>
                    <Button x:Name="WheatherPg" Text="Погода"
                            Clicked="GetItems_Clicked_3"/>
                    <Button x:Name="InfoPg" Text="Инфо"
                            Clicked="GetItems_Clicked_1"/>
                </StackLayout>
            </ContentPage.Content>
        </ContentPage>
    </MasterDetailPage.Master>
</MasterDetailPage>